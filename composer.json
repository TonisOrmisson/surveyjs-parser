{
    "name": "collecthor/surveyjs-parser",
    "description": "Parse SurveyJS survey structure into PHP classes",
    "type": "library",
    "license": "mit",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Collecthor\\SurveyjsParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Collecthor\\SurveyjsParser\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sam Mousa",
            "email": "sam@mousa.nl"
        }
    ],
    "require-dev": {
        "symplify/easy-coding-standard": "^9.4",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.2",
        "ramsey/conventional-commits": "1.x-dev",
        "captainhook/plugin-composer": "^5.3",
        "nikic/iter": "dev-master",
        "phpstan/phpstan-phpunit": "1.0.x-dev",
        "phpstan/extension-installer": "dev-master",
        "phpstan/phpstan-strict-rules": "1.1.x-dev",
        "symfony/console": "< 6"
    },
    "require": {
        "php": ">= 8.1",
        "collecthor/data-interfaces": "> 1.0.1"
    },
    "config": {
        "allow-plugins": {
            "captainhook/plugin-composer": true,
            "phpstan/extension-installer": true
        }
    }
}
