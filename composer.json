{
    "name": "collecthor/surveyjs-parser",
    "description": "Parse SurveyJS survey structure into PHP classes",
    "type": "library",
    "license": "mit",
    "minimum-stability": "stable",
    "autoload": {
        "files": [
            "src/Helpers/utils.php"
        ],
        "psr-4": {
            "Collecthor\\SurveyjsParser\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/support/functions.php"
        ],
        "psr-4": {
            "Collecthor\\SurveyjsParser\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sam Mousa",
            "email": "sam@mousa.nl"
        }
    ],
    "require-dev": {
        "symplify/easy-coding-standard": ">= 11",
        "phpunit/phpunit": "^12",
        "phpstan/phpstan": "^2",
        "ramsey/conventional-commits": "^1",
        "captainhook/plugin-composer": "^5.3",
        "nikic/iter": "^2",
        "phpstan/extension-installer": "^1",
        "phpstan/phpstan-strict-rules": "^2"
    },
    "require": {
        "php": ">= 8.4"
    },
    "config": {
        "allow-plugins": {
            "captainhook/plugin-composer": true,
            "phpstan/extension-installer": true
        }
    }
}
